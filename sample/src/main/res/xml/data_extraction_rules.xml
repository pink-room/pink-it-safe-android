<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup disableIfNoEncryptionCapabilities="true">
        <include
            domain="sharedpref"
            path="." />
        <include
            domain="sharedpref"
            path="safe_storage.xml"
            requireFlags="clientSideEncryption" />
    </cloud-backup>
    <device-transfer>
        <include
            domain="sharedpref"
            path="." />
        <include
            domain="sharedpref"
            path="safe_storage.xml"
            requireFlags="clientSideEncryption" />
    </device-transfer>
</data-extraction-rules>